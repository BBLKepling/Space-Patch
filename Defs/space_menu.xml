<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <XmlExtensions.SettingsMenuDef>
    <defName>BBLK_Space_Menu</defName>
    <modId>BBLK_Space_Patch</modId>
    <label>Space Menu</label>
    <settings>
      <li Class="XmlExtensions.Setting.Slider">
        <key>More_Space_Patch</key>
        <label>Number of Spaces? : {More_Space_Patch}</label>
        <defaultValue>1</defaultValue>
        <min>1</min>
        <max>5</max>
        <decimals>0</decimals>
      </li>
      <li Class="XmlExtensions.Setting.Checkbox">
        <key>Hyper_Patch</key>
        <label>Add Hyper before all the Spaces</label>
        <defaultValue>false</defaultValue>
      </li>
    </settings>
  </XmlExtensions.SettingsMenuDef>

</Defs>